{
  "name": "react-range-progress",
  "version": "4.0.3",
  "description": "Eases styling of sliders (range inputs).",
  "main": "dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "test": "babel-tape-runner src/test.js",
    "test:pretty": "babel-tape-runner src/test.js | faucet",
    "test:nyc": "NODE_ENV=test ./node_modules/nyc/bin/nyc.js -x test.js npm test",
    "coverage": "nyc report --reporter=lcov",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "check-coverage": "nyc check-coverage --functions 50",
    "build": "./node_modules/.bin/babel ./src/index.js -o ./dist/index.js",
    "prepublish": "npm run build",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vgrafe/react-range-progress.git"
  },
  "keywords": [
    "react",
    "range",
    "progress",
    "slider",
    "volume"
  ],
  "author": "Vincent Graf√© <vincent.grafe@gmail.com> (https://github.com/vgrafe)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vgrafe/react-range-progress/issues"
  },
  "homepage": "https://github.com/vgrafe/react-range-progress#readme",
  "dependencies": {
    "react": "15.3.1",
    "react-dom": "15.3.1"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^2.0.0",
    "autoprefixer": "6.4.0",
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-loader": "6.2.5",
    "babel-plugin-istanbul": "2.0.1",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.14.0",
    "babel-tape-runner": "2.0.1",
    "codecov.io": "0.1.6",
    "extend-tape": "1.2.0",
    "faucet": "0.0.1",
    "ghooks": "1.3.2",
    "nyc": "8.1.0",
    "react": "15.3.1",
    "react-addons-test-utils": "15.3.1",
    "react-unit": "2.0.0",
    "semantic-release": "^6.3.6",
    "tape": "4.6.0",
    "tape-jsx-equals": "1.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test:nyc && npm run check-coverage"
    }
  },
  "nyc": {
    "include": [
      "src/index.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  }
}