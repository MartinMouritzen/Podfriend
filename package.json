{
	"name": "podfriend",
	"productName": "PodFriend",
	"version": "0.5.0",
	"description": "PodFriend - Because everyone needs a PodFriend.",
	"license": "UNLICENSED",
	"private": true,
	"scripts": {
		"build": "concurrently \"yarn build-main\" \"yarn build-renderer\"",
		"build-dll": "cross-env NODE_ENV=development webpack --config ./configs/webpack.config.renderer.dev.dll.babel.js --colors",
		"build-e2e": "cross-env E2E_BUILD=true yarn build",
		"build-main": "cross-env NODE_ENV=production webpack --config ./configs/webpack.config.main.prod.babel.js --colors",
		"build-renderer": "cross-env NODE_ENV=production webpack --config ./configs/webpack.config.renderer.prod.babel.js --colors",
		"build-web": "cross-env NODE_ENV=production webpack --mode production --config ./configs/webpack.config.web.prod.babel.js --colors",
		"dev": "cross-env START_HOT=1 node -r @babel/register ./internals/scripts/CheckPortInUse.js && cross-env START_HOT=1 yarn start-renderer-dev",
		"dev-web-crap": "cross-env NODE_ENV=development nodemon ./app/web/web.serve.js",
		"dev-web-old": "cross-env NODE_ENV=development webpack --config ./configs/webpack.config.web.dev.babel.js && nodemon ./app/web/web.serve.js",
		"dev-web": "cross-env NODE_ENV=development webpack-dev-server --config ./configs/webpack.config.web.dev.babel.js",
		"deploy-web": "gsutil rsync -R ./app/web/dist gs://podfriend",
		"package": "yarn build && electron-builder build --publish never",
		"package-all": "yarn build && electron-builder build -mwl",
		"package-ci": "yarn postinstall && yarn build && electron-builder --publish always",
		"package-linux": "yarn build && electron-builder build --linux",
		"package-win": "yarn build && electron-builder build --win --x64",
		"postinstall": "yarn build-dll",
		"prestart": "yarn build",
		"rebuild-native-dependencies": "./node_modules/.bin/electron-rebuild",
		"start": "cross-env NODE_ENV=production electron ./app/main.prod.js",
		"start-main-dev": "cross-env HOT=1 NODE_ENV=development electron -r @babel/register ./app/main.dev.js",
		"start-renderer-dev": "cross-env NODE_ENV=development webpack-dev-server --config configs/webpack.config.renderer.dev.babel.js",
		"bundle-report": "webpack-bundle-analyzer --port 4200 compilation-stats.json"
	},
	"main": "./app/main.prod.js",
	"build": {
		"productName": "PodFriend",
		"appId": "com.podfriend",
		"files": [
			"app/dist/",
			"app/images/",
			"app/app.html",
			"app/main.prod.js",
			"app/main.prod.js.map",
			"app/components/*.jsx",
			"package.json"
		],
		"dmg": {
			"contents": [
				{
					"x": 130,
					"y": 220
				},
				{
					"x": 410,
					"y": 220,
					"type": "link",
					"path": "/Applications"
				}
			]
		},
		"win": {
			"publish": [
				{
					"provider": "generic",
					"url": "http://update.podfriend.com"
				}
			],
			"target": [
				"nsis",
				"msi"
			]
		},
		"linux": {
			"target": [
				"deb",
				"rpm",
				"snap",
				"AppImage"
			],
			"category": "Development"
		},
		"directories": {
			"buildResources": "resources",
			"output": "release"
		},
		"publish": {
			"provider": "github",
			"owner": "MartinMouritzen",
			"repo": "Podfriend",
			"private": true
		}
	},
	"repository": {
		"type": "git",
		"url": ""
	},
	"author": {
		"name": "Martin Mouritzen",
		"email": "martin@martinmouritzen.dk",
		"url": "https://www.podfriend.com"
	},
	"contributors": [
		{
			"name": "Martin Mouritzen",
			"email": "martin@martinmouritzen.dk",
			"url": "https://www.podfriend.com"
		}
	],
	"keywords": [
		"electron",
		"boilerplate",
		"react",
		"redux",
		"flow",
		"sass",
		"webpack",
		"hot",
		"reload"
	],
	"homepage": "https://www.podfriend.com",
	"jest": {
		"testURL": "http://localhost/",
		"moduleNameMapper": {
			"\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/internals/mocks/fileMock.js",
			"\\.(css|less|sass|scss)$": "identity-obj-proxy"
		},
		"moduleFileExtensions": [
			"js",
			"jsx",
			"json"
		],
		"transform": {
			"^.+\\.jsx?$": "babel-jest"
		},
		"setupFiles": [
			"./internals/scripts/CheckBuiltsExist.js"
		]
	},
